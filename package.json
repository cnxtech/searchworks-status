{
  "name": "searchworks-status",
  "version": "1.0.0",
  "description": "A dashboard for searchworks status checks.",
  "engines": {
    "npm": ">=6"
  },
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --mode development",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "lint": "npx eslint src/ --ext jsx --ext js",
    "test": "jest",
    "test:watch": "jest --watch",
    "report-coverage": "node_modules/.bin/jest --coverage | codecov"
  },
  "author": "aeschylus",
  "license": "ISC",
  "devDependencies": {
    "acorn": "^6.1.0",
    "acorn-jsx": "^5.0.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.1.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "espree": "^4.1.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.22.1",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.21",
    "prop-types": "^15.7.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-hot-loader": "^4.6.5",
    "react-twitter-embed": "^1.1.3"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupEnzyme.js",
    "roots": [
      "./__tests__/",
      "src/"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**"
    ]
  }
}
